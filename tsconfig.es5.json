{
  "compilerOptions": {
    "module": "es6",
    "moduleResolution": "node",
    "target": "es5",
    "lib":["DOM","ES5","ScriptHost","ES2015.Promise"],
    "sourceMap": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors":true,
    "declaration":false,
    "outDir":"build/es5/",
    "rootDir":"src/ts/"
  },
  "exclude": [
    "node_modules"
  ]
}
